{
  "name": "river-settler",
  "version": "0.0.1",
  "description": "game rendered using threejs",
  "main": "server.js",
  "scripts": {
    "start": "./run.sh",
    "test": "karma start",
    "serve": "cd public && concurrently 'watchify js/main.js -d -t [babelify] -o js/dist/bundle.js -v' 'browser-sync start --open external --server --files \" js/dist/bundle.js\"'",
    "model": "./run-model.sh"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jsimmons--dev/river-settler.git"
  },
  "keywords": [
    "threejs"
  ],
  "author": "Joshua Simmons, Corey Shannon",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Jsimmons--dev/river-settler/issues"
  },
  "homepage": "https://github.com/Jsimmons--dev/river-settler#readme",
  "dependencies": {
    "babel-loader": "^7.0.0",
    "karma": "^1.7.0",
    "karma-chrome-launcher": "^2.1.1",
    "karma-jasmine": "^1.1.0",
    "karma-webpack": "^2.0.3",
    "orbit-controls-es6": "^1.0.8",
    "three": "^0.85.2",
    "webpack": "^2.6.1"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babelify": "^7.2.0",
    "jasmine": "^2.6.0",
    "jasmine-core": "^2.6.2",
    "karma-sourcemap-loader": "^0.3.7"
  }
}
